var canvaz= Document.getElementById("putafoto");
var ctxq= canvaz.getContext("2d");
window.onload = init;
var imagen= new Imagen();



function init(){
pintar();
bola();
}

function pintar(){
    imagen.src="../images/FLECHA_ABAJO.png";
    ctxq.drawImagen(imagen,0,0);
}
function bola(){
    ctxq.beginPath();
  ctxq.arc(bol_disparoX, bol_disparoY, borde, 0, 2 * Math.PI);
  ctxq.fillStyle = "white";
  ctxq.fill();
  ctxq.closePath();
}